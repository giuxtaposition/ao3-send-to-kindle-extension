{
    "manifest_version": 2,
    "name": "ao3-send-to-kindle",
    "version": "1.0.0",
    "description": "A simple extension to send AO3 stories to your Kindle",
    "permissions": ["storage"],
    "browser_action": {
        "default_icon": {
            "19": "Archive_of_Our_Own_logo.png",
            "38": "Archive_of_Our_Own_logo.png"
        },
        "default_popup": "popup.html"
    }
}
